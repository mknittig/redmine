<h2><%=l(:label_enumerations)%></h2>

<% form_tag(enumeration_path(@enumeration), :method => :put, :class => "tabular") do %>
  <%= render :partial => 'form' %>
  <%= submit_tag l(:button_save) %>
<% end %>

<% form_tag(enumeration_path(@enumeration), :method => :delete) do %>
  <%= submit_tag l(:button_delete) %>
<% end %>